    <!-- Google Analytics 4 -->
    <script async src="https://www.googletagmanager.com/gtag/js?id=G-BOTINC2025"></script>
    <script>
        window.dataLayer = window.dataLayer || [];
        function gtag(){dataLayer.push(arguments);}
        gtag('js', new Date());
        gtag('config', 'G-BOTINC2025', {
            custom_map: {
                'custom_parameter_1': 'game_type',
                'custom_parameter_2': 'player_level',
                'custom_parameter_3': 'session_length'
            }
        });
        
        // Enhanced Analytics Functions
        window.trackGameEvent = function(action, game, details = {}) {
            gtag('event', action, {
                'event_category': 'game_interaction',
                'event_label': game,
                'custom_parameter_1': details.game_type || 'browser_game',
                'custom_parameter_2': details.player_level || '1',
                'custom_parameter_3': details.session_length || '0',
                'value': details.score || 0,
                'engagement_time_msec': details.engagement_time || 0
            });
        };
        
        window.trackSocialShare = function(platform, game, score) {
            gtag('event', 'share', {
                'method': platform,
                'content_type': 'game_score',
                'content_id': game,
                'value': score
            });
        };
        
        window.trackAchievement = function(achievement_id, game, value) {
            gtag('event', 'unlock_achievement', {
                'achievement_id': achievement_id,
                'event_category': 'achievements',
                'event_label': game,
                'value': value
            });
        };
        
        // Session tracking
        let sessionStartTime = Date.now();
        let engagementEvents = [];
        
        window.trackEngagement = function(eventType, data = {}) {
            let eventTime = Date.now();
            let sessionTime = Math.floor((eventTime - sessionStartTime) / 1000);
            
            engagementEvents.push({
                event: eventType,
                time: sessionTime,
                data: data
            });
            
            gtag('event', 'custom_engagement', {
                'event_category': 'user_engagement',
                'event_label': eventType,
                'session_time': sessionTime,
                'total_events': engagementEvents.length
            });
        };
    </script>
